/* pages/mydiscount/mydiscount.wxss */
page{
  background: #f0eff4;
}
.img{
  width: 272rpx;
  height: 226rpx;
  margin: 0 auto;
  border: 1px dashed #888888;
  margin-top: 30%;
}
.img>image{
  width: 100%;
  height: 100%;
}
.text{
  text-align: center;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #666666;
  line-height: 60rpx;
}
.title{
  display: flex;
  flex-wrap: nowrap;
}
.center{
  display: flex;
  justify-content:space-between;
}
.Tab{
  position: fixed;
  width: 100%;
  top: 0;
}
.top_tab{
  margin-top: 85rpx;
}
.discount_item{
  margin: 30rpx auto;
  margin-bottom: 0;
  width: 686rpx;
  background:#fff;
}
.discount_item_top{
  display: flex;
  justify-content: space-between;
  padding:21rpx 23rpx;
  box-sizing: border-box;
  border-bottom: 1px dashed #999999;
  
}
.discount_item_image{
  display: flex;
  justify-content: flex-start;

}
.discount_item_image>image{
  width: 126rpx;
  height: 110rpx;
  margin-right: 30rpx;

}
.discount_item_image_title{

  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #333333;
  padding-top: 10rpx;
}
.discount_item_image_title>view:nth-child(2){
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #666666;
  margin-top: 10rpx;
}
.discount_item_price{
  line-height: 110rpx;

  font-size: 48rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #EE3137;

}
.discount_item_bottom{
  display: flex;
  justify-content: space-between;
  padding: 0 50rpx;
  box-sizing: border-box;

  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #666666;
  height: 85rpx;
  line-height: 85rpx;

}
.famousDoctors_bottom{
  position: fixed;
  bottom: 0;
  left: 0;
  background: #fff;
  padding:12rpx 36rpx;
}
.famousDoctors_bottom_btn{
  width: 678rpx;
  height: 74rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #FFFFFF;
  text-align: center;
  line-height: 74rpx;
  background: #4FA7FF;
}
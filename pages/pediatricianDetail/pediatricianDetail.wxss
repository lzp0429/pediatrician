/* pages/pediatricianDetail/pediatricianDetail.wxss */
@import "/wxParse/wxParse.wxss";
.title{
  text-align: center;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #000000;
  width: 60%;
  margin: 0 auto;
  margin-top: 30rpx;
}
.center{
  width: 90%;
  margin: 0 auto; 
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 36rpx;
  margin-top: 30rpx;
}
img{
  width: 90%;
}
.bottom_btn{
  position: fixed;
  right: 32rpx;
  bottom: 280rpx;
}
.bottom_btn>view{
  background: #4FA7FF;
  border-radius:125rpx;
  width: 72rpx;
  height: 72rpx;
  padding: 16rpx;
  box-sizing: border-box;

}
.bottom_btn>view:nth-child(2){
  margin-top: 20rpx;
}
.bottom_btn>view>image{
  width: 40rpx;
  height: 40rpx;

}